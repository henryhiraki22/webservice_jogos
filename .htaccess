RewriteEngine On
RewriteRule ^/?([a-zA-Z_]+)/([a-zA-Z_]+)/([0-9]+)$ web/web.php?metodo=$1&arg1=$2&arg2=$3
RewriteRule ^/?([a-zA-Z_]+)/([0-9]+)$ web/web.php?metodo=$1&arg1=$2 
RewriteRule ^/?([a-zA-Z_]+)$ web/web.php?metodo=$1 
RewriteRule ^/?([a-zA-Z_]+)/([a-zA-Z_]+)$ mvc/app.php?classe=$1&metodo=$2 [L]
